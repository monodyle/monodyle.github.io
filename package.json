{
  "name": "monody-space",
  "version": "3.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "next start",
    "dev": "yarn && next",
    "build": "next build",
    "export": "next export",
    "prepare": "husky install",
    "lint": "eslint --fix .",
    "format": "prettier --write .",
    "precommit": "prettier --write . && eslint --fix ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint"
    }
  },
  "dependencies": {
    "@leafac/rehype-shiki": "^1.3.1",
    "gray-matter": "^4.0.3",
    "lazysizes": "^5.3.2",
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-katex": "^5.0.0",
    "rehype-shiki": "^0.0.9",
    "rehype-slug": "^4.0.1",
    "rehype-stringify": "^8.0.0",
    "remark": "^13.0.0",
    "remark-footnotes": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-html": "^13.0.1",
    "remark-math": "^4.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "remark-slug": "^6.0.0",
    "shiki": "^0.9.5",
    "unified": "^9.2.1"
  },
  "devDependencies": {
    "@types/node": "^15.14.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "autoprefixer": "^10.2.6",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "lint-staged": ">=10",
    "organize-imports-cli": "^0.8.0",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.4",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "organize-imports-cli",
      "prettier --write "
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
